{
  "streams":[
     {
        "stream":{
           "name":"jobs",
           "json_schema":{
            "additionalProperties": true,
              "required":[
                 "jobId",
                 "status",
                 "jobType",
                 "startTime"
              ],
              "type":"object",
              "properties":{
                 "jobId":{
                    "type":"integer"
                 },
                 "status":{
                    "type":"string",
                    "enum":[
                       "pending",
                       "running",
                       "incomplete",
                       "failed",
                       "succeeded",
                       "cancelled"
                    ]
                 },
                 "jobType":{
                    "type":"string",
                    "enum":[
                       "sync",
                       "reset"
                    ]
                 },
                 "startTime":{
                    "type":"string"
                 },
                 "lastUpdatedAt":{
                    "type":"string"
                 },
                 "duration":{
                    "type":"string"
                 },
                 "bytesSynced":{
                    "type":"integer"
                 },
                 "rowsSynced":{
                    "type":"integer"
                 }
              }
           },
           "supported_sync_modes":[
              "full_refresh"
           ]
        },
        "sync_mode":"full_refresh",
        "destination_sync_mode":"overwrite"
     },
     {
        "stream":{
           "name":"workspaces",
           "json_schema":{
            "additionalProperties": true,
              "required":[
                 "workspaceId",
                 "name",
                 "dataResidency"
              ],
              "type":"object",
              "properties":{
                 "workspaceId":{
                    "type":"string"
                 },
                 "name":{
                    "type":"string"
                 },
                 "dataResidency":{
                    "type":"string",
                    "enum":[
                       "eu",
                       "us",
                       "auto"
                    ]
                 }
              }
           },
           "supported_sync_modes":[
              "full_refresh"
           ]
        },
        "sync_mode":"full_refresh",
        "destination_sync_mode":"overwrite"
     },
     {
        "stream":{
           "name":"connections",
           "json_schema":{
            "additionalProperties": true,
              "required":[
                 "workspaceId",
                 "name",
                 "dataResidency"
              ],
              "type":"object",
              "properties":{
                 "connectionId":{
                    "type":"string"
                 },
                 "name":{
                    "type":"string"
                 },
                 "sourceId":{
                    "type":"string"
                 },
                 "workspaceId":{
                    "type":"string"
                 },
                 "destinationId":{
                    "type":"string"
                 },
                 "status":{
                    "type":"string",
                    "enum":[
                       "active",
                       "inactive",
                       "deprecated"
                    ]
                 },
                 "schedule":{
                    "type":"object",
                    "required":[
                       "scheduleType"
                    ],
                    "properties":{
                       "scheduleType":{
                          "type":"string",
                          "enum":[
                             "manual",
                             "cron",
                             "basic"
                          ]
                       },
                       "cronExpression":{
                          "type":"string"
                       },
                       "basicTiming":{
                          "type":"string"
                       }
                    }
                 },
                 "dataResidency":{
                    "type":"string",
                    "enum":[
                       "eu",
                       "us",
                       "auto"
                    ]
                 }
              }
           },
           "supported_sync_modes":[
              "full_refresh"
           ]
        },
        "sync_mode":"full_refresh",
        "destination_sync_mode":"overwrite"
     },
     {
        "stream":{
           "name":"sources",
           "json_schema":{
            "additionalProperties": true,
              "required":[
                 "workspaceId",
                 "name",
                 "sourceId",
                 "sourceType"
              ],
              "type":"object",
              "properties":{
                 "sourceId":{
                    "type":"string"
                 },
                 "name":{
                    "type":"string"
                 },
                 "sourceType":{
                    "type":"string"
                 },
                 "workspaceId":{
                    "type":"string"
                 }
              }
           },
           "supported_sync_modes":[
              "full_refresh"
           ]
        },
        "sync_mode":"full_refresh",
        "destination_sync_mode":"overwrite"
     }
  ]
}